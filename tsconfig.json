{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2015",
      "dom"
    ],
    "declaration": true,
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "typedocOptions": {
    "name": "@json-schema-tools/traverse",
    "mode": "file",
    "src": [
      "./src/index.ts"
    ],
    "out": "docs",
    "includeDeclarations": true,
    "excludeNotExported": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "hideGenerator": true,
    "listInvalidSymbolLinks": true,
    "readme": "./README.md",
    "exclude": [
      "node_modules",
      "lib",
      "docs",
      "build",
      "jest.config.js",
      "src/*.test.ts"
    ],
    "toc": [
      "traverse"
    ]
  }
}
